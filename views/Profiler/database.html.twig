{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% set query_count = collector.queries|length %}

    {% set imageData -%}
        data:image/png;base64,
    {%- endset %}

    {% set icon %}
        <img width="20" height="28" alt="Database" style="border-width: 0; vertical-align: middle; margin-right: 0;" src="{{ imageData }}" />
        <span class="sf-toolbar-status{% if query_count > 0 %} sf-toolbar-status-green{% endif %}">{{ query_count }}</span>
        {% if query_count > 0 %}
            in {{ collector.duration|round }} ms
        {% endif %}
    {% endset %}
    {% set text %}
        <div class="sf-toolbar-info-piece">
            <b>Queries</b>
            <span>{{ query_count }}</span>
        </div>
    {% endset %}
    {% include 'WebProfilerBundle:Profiler:toolbar_item.html.twig' with { 'link': true } %}
{% endblock %}

{% block head %}
    {{ parent() }}
{% endblock %}

{% block menu %}
    {% set imageData -%}
        data:image/png;base64,
    {%- endset %}

    <span class="label">
        <span class="icon">
            <img src="{{ imageData }}" alt="Dependency Injection Container" />
        </span>

        <strong>Database</strong>
        <span class="count">
            <span>{{ collector.queries|length }}</span>
            <span>{{ collector.duration|round }} ms</span>
        </span>
    </span>
{% endblock %}

{% block panel %}
    <h2>Database</h2>

    <ul class="alt">
        {% for query in collector.queries %}
        <li>
            <pre>
                {%- for key, part in query.queryParts -%}
                    {{- part -}}
                    {%- if query.params[key] is defined -%}
                        {%- if query.types[key:1] in ['s', 'b'] -%}
                            <b>"{{ query.params[key]|escape('html_attr') }}"</b>
                        {%- else -%}
                            <b>{{ query.params[key] }}</b>
                        {%- endif -%}
                    {%- endif -%}
                {%- endfor -%}
            </pre> in <b>{{ query.duration|number_format(1) }}</b> &micro;s
            {#{{ query|ladybug_dump() }}#}
        </li>
        {% endfor %}
</ul>
{% endblock %}
